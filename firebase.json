{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "**", "destination": "/index.html" }
    ]
  },
  "redirects": [
  {
    "source": "/",
    "destination": "https://itspossible.co.tz",
    "type": 301
  }
]
}
